{
  "name": "susie-cmy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:clean": "rimraf .next && next build",
    "build:fresh": "rimraf .next node_modules/.cache && pnpm run build",
    "clean": "rimraf .next out node_modules/.cache",
    "clean:all": "rimraf .next out node_modules/.cache node_modules && pnpm install",
    "build:prod": "NODE_ENV=production pnpm run clean && next build",
    "export": "next build && next export",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "@types/react-syntax-highlighter": "^15.5.13",
    "animejs": "^4.0.2",
    "gray-matter": "^4.0.3",
    "next": "15.3.5",
    "react": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.4.5",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.0.0",
    "daisyui": "^5.0.46",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "react-use": "^17.6.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4",
    "typescript": "^5",
    "zustand": "^5.0.6"
  }
}
